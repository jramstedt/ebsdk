                  [0058C0]  6639  ;
                  [0058C0]  6640  ; Function:
                  [0058C0]  6641  ;	Execute indicated performance monitoring function.
                  [0058C0]  6642  ;
                  [0058C0]  6643  ; Exit state:
                  [0058C0]  6644  ;	r0(v0)		Value returned for read
                  [0058C0]  6645  ;	r17(a1)		UNPREDICTABLE
                  [0058C0]  6646  ;-
                  [0058C0]  6647  
                  [0058C0]  6648  
                  [0058C0]  6649  ;-----------------------------------------------------------------------
                  [0058C0]  6650  ;	Arguments and Actions
                  [0058C0]  6651  ;-----------------------------------------------------------------------
                  [0058C0]  6652  ;	r16(a0) = 0		Disable performance monitoring
                  [0058C0]  6653  ;	r17(a1) = bitmask	r17(a1)<0>=1	disable CTR0
                  [0058C0]  6654  ;				r17(a1)<1>=1	disable CTR1
                  [0058C0]  6655  ;
                  [0058C0]  6656  ;	Action: Clear PCTx_EN in I_CTL as directed. If CTRx is already
                  [0058C0]  6657  ;		disabled, it stays disabled.
                  [0058C0]  6658  ;-----------------------------------------------------------------------
                  [0058C0]  6659  ;	r16(a0) = 1		Enable performance monitoring
                  [0058C0]  6660  ;	r17(a1) = bitmask	r17(a1)<0>=1	enable CTR0
                  [0058C0]  6661  ;				r17(a1)<1>=1	enable CTR1
                  [0058C0]  6662  ;
                  [0058C0]  6663  ;	Action: Set PCTx_EN in I_CTL as directed. If CTRx is already
                  [0058C0]  6664  ;		enabled, it stays enabled.
                  [0058C0]  6665  ;-----------------------------------------------------------------------
                  [0058C0]  6666  ;	r16(a0) = 2		Mux select
                  [0058C0]  6667  ;	r17(a1) = pctr_ctl	r17(a1)<3:0>	SL1 pass 2.3 (see table)
                  [0058C0]  6668  ;				r17(a1)<3:2>	SL1 pass 3   (see table)
                  [0058C0]  6669  ;				r17(a1)<4>	SL0 (see table below)
                  [0058C0]  6670  ;	Action: Write PCTR_CTL SLx fields.
                  [0058C0]  6671  ;-----------------------------------------------------------------------
                  [0058C0]  6672  ;	r16(a0) = 3		Options
                  [0058C0]  6673  ;	r17(a1) = options	r17(a1)<0>	logging option
                  [0058C0]  6674  ;					0 => log all processes
                  [0058C0]  6675  ;					1 => log only selected processes
                  [0058C0]  6676  ;	Action: If r17(a1)<0> is clear, set SPCE bit in I_CTL
                  [0058C0]  6677  ;-----------------------------------------------------------------------
                  [0058C0]  6678  ;	r16(a0) = 5		Read counters
                  [0058C0]  6679  ;
                  [0058C0]  6680  ;	Action: Return in r0(v0) the counters of PCTR_CTL
                  [0058C0]  6681  ;-----------------------------------------------------------------------
                  [0058C0]  6682  ;	r16(a0) = 6		Write counters
                  [0058C0]  6683  ;	r17(a1)			r17(a1)<0> Write counter 0
                  [0058C0]  6684  ;				r17(a1)<1> Write counter 1
                  [0058C0]  6685  ;				r17(a1)<25:6>  new pctr1 value
                  [0058C0]  6686  ;				r17(a1)<47:28> new pctr0 value
                  [0058C0]  6687  ;	Action: Selectively write counter fields in PCTR_CTL
                  [0058C0]  6688  ;-----------------------------------------------------------------------
                  [0058C0]  6689  ;	r16(a0) = 7		Enable and write selected counters
                  [0058C0]  6690  ;	r17(a1) = bitmask	r17(a1)<0>=1	enable and clear PCTR0
                  [0058C0]  6691  ;				r17(a1)<1>=1	enable and clear PCTR1
                  [0058C0]  6692  ;
                  [0058C0]  6693  ;	Action: Write specified counter(s) as directed
                  [0058C0]  6694  ;		Set PCTx_EN in I_CTL as directed
                  [0058C0]  6695  ;		User requests a clear by having 0 in the counter fields
                  [0058C0]  6696  ;-----------------------------------------------------------------------
                  [0058C0]  6697  ;	r16(a0) = 8		Read i_stat (ProfileMe)
                  [0058C0]  6698  ;
                  [0058C0]  6699  ;	Action: Return in r0(v0) the i_stat values
                  [0058C0]  6700  ;-----------------------------------------------------------------------
                  [0058C0]  6701  ;	r16(a0) = 9		Read pmpc (ProfileMe)
                  [0058C0]  6702  ;
                  [0058C0]  6703  ;	Action: Return in r0(v0) the PC of the last profiled instruction
                  [0058C0]  6704  ;-----------------------------------------------------------------------
                  [0058C0]  6705  ;-----------------------------------------------------------------------
                  [0058C0]  6706  ; Pass 2.3 Aggregate mode
                  [0058C0]  6707  ;
                  [0058C0]  6708  ; SL0 mux values
                  [0058C0]  6709  ;	0	PCTR0 <- cycles
                  [0058C0]  6710  ;	1	PCTR0 <- retired instructions
                  [0058C0]  6711  ; SL1 mux values
                  [0058C0]  6712  ;	0000 	PCTR1 <- cycles
                  [0058C0]  6713  ;	0001 	PCTR1 <- retired conditional branches
                  [0058C0]  6714  ;	0010 	PCTR1 <- cycles (retired branch mispredicts not implemented)
                  [0058C0]  6715  ;	0011 	PCTR1 <- retired dtb single misses * 2 (bats 943)
                  [0058C0]  6716  ;	0100 	PCTR1 <- retired dtb double misses
                  [0058C0]  6717  ;	0101 	PCTR1 <- retired itb misses
                  [0058C0]  6718  ;	0110 	PCTR1 <- retired unaligned traps
                  [0058C0]  6719  ;	0111 	PCTR1 <- replay traps
                  [0058C0]  6720  ;-----------------------------------------------------------------------
                  [0058C0]  6721  ; Pass 3 or higher Aggregate mode
                  [0058C0]  6722  ;
                  [0058C0]  6723  ; SL0 mux values
                  [0058C0]  6724  ;	0	aggregate mode
                  [0058C0]  6725  ; SL1 mux values
                  [0058C0]  6726  ;	00	PCTR0 <- retired  PCTR1 <- cycles
                  [0058C0]  6727  ;	01	PCTR0 <- cycles	  PCTR1 <- undefined
                  [0058C0]  6728  ;	10	PCTR0 <- retired  PCTR1 <- bcache misses/long probe latency
                  [0058C0]  6729  ;	11	PCTR0 <- cycles	  PCTR1 <- Mbox replay traps
                  [0058C0]  6730  ;-----------------------------------------------------------------------
                  [0058C0]  6731  ; Pass 3 or higher ProfileMe mode
                  [0058C0]  6732  ;
                  [0058C0]  6733  ; SL0 mux values
                  [0058C0]  6734  ;	1	ProfileMe mode
                  [0058C0]  6735  ; SL1 mux values
                  [0058C0]  6736  ;	00	PCTR0 <- retired  PCTR1 <- cycles
                  [0058C0]  6737  ;	01	PCTR0 <- cycles	  PCTR1 <- cycles of delayed retire ptr advance
                  [0058C0]  6738  ;	10	PCTR0 <- retired  PCTR1 <- bcache misses/long probe latency
                  [0058C0]  6739  ;	11	PCTR0 <- cycles	  PCTR1 <- Mbox replay traps
                  [0058C0]  6740  ;-----------------------------------------------------------------------
                  [0058C0]  6741  ;-----------------------------------------------------------------------
                  [0058C0]  6742  ; Note that the PPCE (per process counter enable) bit in PCTX, is
                  [0058C0]  6743  ; set/cleared on swap context, and does not get changed as a result of
                  [0058C0]  6744  ; a perfmon call.
                  [0058C0]  6745  ;-----------------------------------------------------------------------
