                  [007168]  3143  ; sys__cbox
                  [007168]  3144  ;
                  [007168]  3145  ; Shift in cbox error register chain.
                  [007168]  3146  ;
                  [007168]  3147  ; External bus activity must be isolated from writes to the CBOX CSR.
                  [007168]  3148  ; This requires that all dstream and istream fills must be avoided until after
                  [007168]  3149  ; the MTPR update completes. An MB instruction can block dstream activity, but
                  [007168]  3150  ; blocking all istream fills including prefetches requires more extensive code.
                  [007168]  3151  ; Also, we assume that i_ctl<sbe>=3 in the normal case, and restore to that
                  [007168]  3152  ; value.
                  [007168]  3153  ;
                  [007168]  3154  ; The 'error' status is cleared when the first mfpr retires.
                  [007168]  3155  ;
                  [007168]  3156  ; We are going to shift 66 bits, even though the 4-bit cbox status field
                  [007168]  3157  ; is pass2 or later.
                  [007168]  3158  ;
                  [007168]  3159  ; Pass1
                  [007168]  3160  ; -----
                  [007168]  3161  ;  8	dc1_syndrome<7:0>	for the last fill scrubbed by dcache ECC checkers
                  [007168]  3162  ;  8	dc0_syndrome<7:0>	for the last fill scrubbed by dcache ECC checkers
                  [007168]  3163  ;  5	c_stat<4:0>		error status
                  [007168]  3164  ; 37	c_addr<6:42>		block address of last reported error (inverted!)
                  [007168]  3165  ;  2	raz<1:0>		padded zeros to get multiple of 6
                  [007168]  3166  ; --
                  [007168]  3167  ; 60 bits
                  [007168]  3168  ;
                  [007168]  3169  ; Pass2
                  [007168]  3170  ; -----
                  [007168]  3171  ;  8	dc1_syndrome<7:0>	for the last fill scrubbed by dcache ECC checkers
                  [007168]  3172  ;  8	dc0_syndrome<7:0>	for the last fill scrubbed by dcache ECC checkers
                  [007168]  3173  ;  5	c_stat<4:0>		error status
                  [007168]  3174  ;  4	c_sts<3:0>		captured status of bcache in init-mode
                  [007168]  3175  ; 37	c_addr<6:42>		block address of last reported error (inverted!)
                  [007168]  3176  ;  4	raz<3:0>		padded zeros to get multiple of 6
                  [007168]  3177  ; --
                  [007168]  3178  ; 66 bits
                  [007168]  3179  ;
                  [007168]  3180  ; Input:
                  [007168]  3181  ;	p5		return address
                  [007168]  3182  ;
                  [007168]  3183  ; Register Use:
