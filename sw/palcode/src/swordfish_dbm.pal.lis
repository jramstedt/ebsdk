                  [006180]    42  ; = keypressed - detect if user pressed a key				=
                  [006180]    43  ; ======================================================================
                  [006180]    44  ; OVERVIEW:
                  [006180]    45  ;       Returns a 1 if a key has been pressed, 0 if not.
                  [006180]    46  ;       check the serial line interrupt bit, if it's set, clear it
                  [006180]    47  ;       and return 1, otherwise, return 0
                  [006180]    48  ;
                  [006180]    49  ; FORM_OF_CALL:
                  [006180]    50  ;       bsr p20, keypressed
                  [006180]    51  ;  
                  [006180]    52  ; RETURNS:
                  [006180]    53  ;       R0  - 1 if keypressed, else 0
                  [006180]    54  ; 
                  [006180]    55  ; SIDE EFFECTS:
                  [006180]    56  ;       The Serial Line Interrupt bit is cleared.
                  [006180]    57  ;
                  [006180]    58  ; ARGUMENTS:
                  [006180]    59  ;       none
                  [006180]    60  ;
                  [006180]    61  ; REGISTERS:
                  [006180]    62  ;       R0  - scratch & return value
                  [006180]    63  ;       R20 - return address 
                  [006180]    64  ;       
                  [006180]    65  ; CALLS:
                  [006180]    66  ;       none
                  [006180]    67  ;
                  [006180]    68   
                  [006180]    69  keypressed:
                  [006180]    70  	hw_mfpr	r0, ev6__isum 			; read Interrupt Summary Register 
                  [006180]    71  	srl	r0, #ev6__isum__sl__s, r0	; r0<0> <- Serial Line Intrpt bit 
                  [006180]    72  	and	r0, #ev6__isum__sl__m, r0	; :				 
                  [006180]    73  	sll	r0, #ev6__hw_int_clr__sl__s, r0	; put into position		 
                  [006180]    74  	PVC_VIOLATE <11>			; EJS -- ignore lack of stall
                  [006180]    75  	PVC_VIOLATE <35>			; EJS -- ignore not writing twice
                  [006180]    76  	hw_mtpr	r0, ev6__hw_int_clr		; Clear any serial line intrpt 
                  [006180]    77  	cmovne	r0, #1, r0			; Return 1 if a key was pressed  
                  [006180]    78  
                  [006180]    79  	PVC_VIOLATE <29>
                  [006180]    80  	PVC_JSR	key, bsr=1, dest=1
                  [006180]    81  	ret	r31, (p20)
