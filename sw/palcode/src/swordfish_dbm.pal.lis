                  [007464]  3704  ; System Reset code for non-srm systems.
                  [007464]  3705  ;
                  [007464]  3706  ; This code starts off as implementation dependent, because it must rely
                  [007464]  3707  ; on a signature to determine whether it came from the SROM. If it came
                  [007464]  3708  ; from the SROM, it does the normal initialization. If it didn't come from
                  [007464]  3709  ; the SROM, it needs to differentiate between a swap and a jump through 0.
                  [007464]  3710  ;
                  [007464]  3711  ; Check for signature. R19<31:16> contains the signature 0xDEC<letter>,
                  [007464]  3712  ; So just check <31:20> for 0xDEC.
                  [007464]  3713  ;
                  [007464]  3714  ; Assume also the absence of the signature says that we ABSOLUTELY
                  [007464]  3715  ;	did not come from a power up. Then I can check p_misc for
                  [007464]  3716  ;	a swap request.
                  [007464]  3717  ;
                  [007464]  3718  ; Entry:		on POWERUP		on SWITCH
                  [007464]  3719  ;
                  [007464]  3720  ;	r1		dc_ctl
                  [007464]  3721  ;	r2 (API ext)	SROM autobaud calibration
                  [007464]  3722  ;			(SROM v16 and up)
                  [007464]  3723  ;	r4 (API ext)	Behavioural code
                  [007464]  3724  ;			(SROM v16 and up)
                  [007464]  3725  ;
                  [007464]  3726  ;	r15		srom_rev
                  [007464]  3727  ;	r16		proc_id
                  [007464]  3728  ;	r17		mem_size		new PC
                  [007464]  3729  ;	r18		cycle_cnt		new PCBB
                  [007464]  3730  ;	r19		signature in <31:16>	new VPTB
                  [007464]  3731  ;	r20		proc_mask
                  [007464]  3732  ;			(note: we are stepping on shadow!)
                  [007464]  3733  ;	r21		sysctx
                  [007464]  3734  ;			(note: we are stepping on p_temp shadow!)
                  [007464]  3735  ;	p22					p_misc switch bit set
                  [007464]  3736  ;	r26		pc+4 of pal base
