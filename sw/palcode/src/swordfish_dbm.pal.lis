                  [006180]    85  ; = getChar -- routine to receive a character from the serial line	=
                  [006180]    86  ; ======================================================================
                  [006180]    87  ; OVERVIEW:
                  [006180]    88  ;       Once the start bit of '0' is detected we wait 1 1/2 bit times 
                  [006180]    89  ;       before reading the first data bit.  The data bit is packed into a 
                  [006180]    90  ;       register and shifted left.  After the first bit we only wait 
                  [006180]    91  ;       whole bit times.  After 8 bits have been read the routine is exited.
                  [006180]    92  ;
                  [006180]    93  ; FORM OF CALL:
                  [006180]    94  ;       bsr	p7, getChar
                  [006180]    95  ; 
                  [006180]    96  ; RETURNS:
                  [006180]    97  ;       R0  - Character returned after user input
                  [006180]    98  ;       
                  [006180]    99  ; CALLS:
                  [006180]   100  ;	waitBitEighth
                  [006180]   101  ;       waitBitHalf
                  [006180]   102  ;       waitBitTime
                  [006180]   103  ;	keypressed
                  [006180]   104  ;       
                  [006180]   105  ; REGISTERS:
                  [006180]   106  ;       R1  - Scratch.
                  [006180]   107  ;       R2  - Bit counter AND character acumulator.
                  [006180]   108  ;             Bits <0:7> is the bit counter (with an extra value of 8)
                  [006180]   109  ;             Bits <8:15> is the character typed in.
                  [006180]   110  ;	R3  - Scratch.
                  [006180]   111  ;       R20 - return address for the call to waitBitTime()
                  [006180]   112  ;
                  [006180]   113   
                  [006180]   114  getChar:
                  [006180]   115  	lda	r2, <0+8>(r31)			; Set the bit count to 0.	 
                  [006180]   116  
                  [006180]   117  	bis	r31, #<20*8>, r3		; Character timeout 20 chars
                  [006180]   118  
                  [006180]   119  ;
                  [006180]   120  ; Wait for the start bits (sl_rcv bit will go low)		 
                  [006180]   121  ;
                  [006180]   122  char_start:
                  [006180]   123  	hw_mfpr	r0, ev6__i_ctl			; Poll the serial line		
                  [006180]   124          srl     r0, #ev6__i_ctl__sl_rcv__s, r0	;
                  [006180]   125  	blbc	r0, start_bit			; until low -> start bit is '0'
                  [006180]   126  
                  [006180]   127  	PVC_JSR	wait, bsr=1
                  [006180]   128  	bsr	p20, waitBitEighth		; Wait for a Eighth bit time	
                  [006180]   129  	subq	r3, #1, r3			; decrement timeout
                  [006180]   130  
                  [006180]   131  	PVC_VIOLATE <1008>			; EJS -- don't follow branch
                  [006180]   132  	bne	r3, char_start			; branch until timeout reached
                  [006180]   133  	lda	r0, -1(r31)			; Return failure code
                  [006180]   134  
                  [006180]   135  	PVC_VIOLATE <29>
                  [006180]   136  	PVC_JSR	getchar, bsr=1, dest=1
                  [006180]   137  	ret	r31, (p7)			; return to caller
                  [006180]   138  
                  [006180]   139  start_bit:
                  [006180]   140  
                  [006180]   141  	PVC_JSR	wait, bsr=1
                  [006180]   142  	bsr	p20, waitBitHalf		; Wait for a half bit time	 
                  [006180]   143  
                  [006180]   144  get_bits:					; DO			 
                  [006180]   145  	PVC_JSR wait, bsr=1
                  [006180]   146  	bsr	p20, waitBitTime		; : wait for one bit time	
                  [006180]   147  
                  [006180]   148  	hw_mfpr	r0, ev6__i_ctl			; : Get the current data bit.	
                  [006180]   149  	srl 	r0, #ev6__i_ctl__sl_rcv__s, r0	; : r0<0> <- current data bit	
                  [006180]   150          and     r0, #1, r0			; :
                  [006180]   151  	sll	r0, r2, r0			; : r0<r2> <- current data bit  
                  [006180]   152  	bis	r0, r2, r2			; : r2 <- r2 .bit_or. r0	
                  [006180]   153  	lda	r2, 1(r2)			; : Increment bit countr.	
                  [006180]   154  	and	r2, #^xFF, r0			; : Ignore character in comp.	
                  [006180]   155  	cmpult	r0, #16, r1			; : Do we have a byte now?
                  [006180]   156  	PVC_VIOLATE <1008>			; EJS -- don't follow branch
                  [006180]   157  	bne	r1, get_bits			; WHILE bit count .NE. 8	
                  [006180]   158  
                  [006180]   159  	PVC_JSR wait, bsr=1
                  [006180]   160  	bsr	p20, waitBitTime		; Wait for the middle of the
                  [006180]   161  						; stop bit.
                  [006180]   162  	PVC_JSR	key, bsr=1
                  [006180]   163  	bsr	p20, keypressed			; Clear serial line interrupts	
                  [006180]   164  
                  [006180]   165  	extbl	r2, #1, r0			; Place character in R0.
                  [006180]   166  	srl	r24, #mc_v_echo, r1		; IF echo_flag set
                  [006180]   167  	blbc	r1, exit_getchar		; THEN			
                  [006180]   168  	xor	r0, #ascii_X, r1		; : IF user sent 'X'	 
                  [006180]   169  	bne	r1, exit_getchar		; : THEN		 
                  [006180]   170  	lda	r1, mc_m_echo(r31)
                  [006180]   171  	bic	r24, r1, r24			; : : clear the echo flag	
                  [006180]   172  						; : ENDIF
                  [006180]   173  exit_getchar:					; ENDIF			 
                  [006180]   174  	PVC_VIOLATE <29>
                  [006180]   175  	PVC_JSR	getchar, bsr=1, dest=1
                  [006180]   176  	ret	r31, (p7)			; return to caller		 
                  [006180]   177  
                  [006180]   178  
                  [006180]   179  ; ======================================================================
                  [006180]   180  ; = putChar - routine to send a character over the serial line		=
                  [006180]   181  ; ======================================================================
                  [006180]   182  ; OVERVIEW:
                  [006180]   183  ;  	This routine is used to transmit a character over the serial line.  
                  [006180]   184  ; 	First a start bit of '0' is sent.  This is followed by eight data 
                  [006180]   185  ;       bits.  The data bits are passed to the routine through a gpr.  The 
                  [006180]   186  ; 	data is transmitted one bit at a time and the gpr is shifted once 
                  [006180]   187  ;       to the right after each bit.  After the last data bit is transmitted
                  [006180]   188  ;       a stop bit of '1' is added to the end of the character.  Extra
                  [006180]   189  ;      stop bits are added to make sure that we don't overflow the
                  [006180]   190  ;      receiving terminal's buffer. 
                  [006180]   191  ;       We make sure that the transmit is initially high so that the
                  [006180]   192  ;       receiver can detect the start bit.
                  [006180]   193  ;
                  [006180]   194  ; FORM OF CALL:
                  [006180]   195  ;       R17	<- the character to be sent. 
                  [006180]   196  ;       bsr	p7, putChar
                  [006180]   197  ;       
                  [006180]   198  ; RETURNS:
                  [006180]   199  ;      none
                  [006180]   200  ;       
                  [006180]   201  ; ARGUMENTS:
                  [006180]   202  ;       R17 - the character which is to be sent to the SROM port.
                  [006180]   203  ;
                  [006180]   204  ; REGISTERS:
                  [006180]   205  ;	r0  - scratch
                  [006180]   206  ;	r1  - scratch
                  [006180]   207  ;       r2  - scratch, bit count
                  [006180]   208  ;       r3  - scratch, I_CTL value
                  [006180]   209  ;       r17 - character to output through the SROM port
                  [006180]   210  ;       p20 - return address for the call to waitBitTime()
                  [006180]   211  ;
                  [006180]   212   
                  [006180]   213  putChar:
                  [006180]   214  	srl	r24, #mc_v_echo, r2		; IF echo flag is clear THEN	 
                  [006180]   215  	blbc	r2, exit_putChar		; Output is disabled, exit	 
                  [006180]   216  ;
                  [006180]   217  ; Initialize routine variables	 
                  [006180]   218  ;
                  [006180]   219  	lda	r2, 16(r31)			; 1 high + 1 start + 8 data + 	 
                  [006180]   220  						; 6 stop (5 extra for delay).   
                  [006180]   221  	lda	r17, ^x3F00(r17)		; add stop bits at the end and	 
                  [006180]   222  	s4addq	r17, #1, r17			; then add start and high bit	 
                  [006180]   223  
                  [006180]   224  data_bit:					; REPEAT			
                  [006180]   225  	hw_mfpr	r0, ev6__i_ctl			; get current I_CTL value	
                  [006180]   226  	lda	r1, 1(r31)			; r1 = mask for sl_xmit bit	
                  [006180]   227  	sll	r1, #ev6__i_ctl__sl_xmit__s, r1 ; move to correct position 
                  [006180]   228  	bic	r0, r1, r0			; clear sl_xmit bit in old I_CTL value 
                  [006180]   229  	blbc	r17, send_bit			; check the new sl_xmit bit	
                  [006180]   230  	bis	r0, r1, r0			; set sl_xmit bit in old I_CTL value 
                  [006180]   231  send_bit:					; continue			
                  [006180]   232  	hw_mtpr	r0, ev6__i_ctl			; EV6 Send the data bit.	
                  [006180]   233  
                  [006180]   234  	srl	r17, #1, r17			; Shift right, to the next bit. 
                  [006180]   235  	subq	r2, #1, r2			; Decrement the bit count.
                  [006180]   236  
                  [006180]   237  	PVC_JSR	wait, bsr=1
                  [006180]   238  	bsr	p20, waitBitTime		; wait for a bit time	   	 
                  [006180]   239  
                  [006180]   240  	PVC_VIOLATE <1008>			; EJS -- don't follow branch
                  [006180]   241  	bne	r2, data_bit			; UNTIL all data sent		 
                  [006180]   242  
                  [006180]   243  exit_putChar:
                  [006180]   244  	PVC_JSR	putchar, bsr=1, dest=1
                  [006180]   245  	PVC_VIOLATE <29>
                  [006180]   246  	ret	r31, (p7)			; return to caller		 
                  [006180]   247  
                  [006180]   248  
                  [006180]   249  ; ======================================================================
                  [006180]   250  ; = waitBitTime - (waitBitHalf) - wait a bit time (wait 0.5 bit-time)	=
                  [006180]   251  ; ======================================================================
                  [006180]   252  ;
                  [006180]   253  ; OVERVIEW:
                  [006180]   254  ;       Waits a bit-time or half-bit time, depending on the entry point.
                  [006180]   255  ;
                  [006180]   256  ; FORM OF CALL:
                  [006180]   257  ;       bsr p20, waitBittime
                  [006180]   258  ;       bsr p20, waitBithalf
                  [006180]   259  ;       
                  [006180]   260  ; RETURNS:
                  [006180]   261  ;       none
                  [006180]   262  ;
                  [006180]   263  ; ARGUMENTS:
                  [006180]   264  ;       R24	- Upper 32 bits contain CPU cycles per bit.
                  [006180]   265  ;
                  [006180]   266  ; REGISTERS:
                  [006180]   267  ;       R0	- CPU cycle counts per bit
                  [006180]   268  ;       R1	- scratch
                  [006180]   269  ;
                  [006180]   270  ;	R28(AT)	- used by STIG to preserve the existing cycle count
                  [006180]   271  ;
                  [006180]   272   
                  [006180]   273  waitBitTime:
                  [006180]   274  	srl	r24, #32, r0			; Get the timing info.		 
                  [006180]   275  	br	r31, waitBitCont
                  [006180]   276  waitBitHalf:
                  [006180]   277  	srl	r24, #<32+1>, r0		; Divide timing info by 2.	 
                  [006180]   278  	br	r31, waitBitCont
                  [006180]   279  waitBitQuarter:
                  [006180]   280  	srl	r24, #<32+2>, r0		; Divide timing info by 4.	 
                  [006180]   281  	br	r31, waitBitCont
                  [006180]   282  waitBitEighth:
                  [006180]   283  	srl	r24, #<32+3>, r0		; Divide timing info by 8.
                  [006180]   284  waitBitCont:
                  [006180]   285  
                  [006180]   286  wait_init:
                  [006180]   287  
                  [006180]   288  ; STIG - an effort to preserve the cycle count accross cServe uart accesses.
                  [006180]   289  ; this modification doesn't preserve the high order 32 bits because they are
                  [006180]   290  ; not used by the diagnostics.  Any OS that ever thought of using the SROM
                  [006180]   291  ; could have serious problems with this...
                  [006180]   292  
                  [006180]   293  	rpcc	r28				; STIG - preserve counter value
                  [006180]   294  	zapnot	r28, #^x0F, r28			; only want bottom 32 bits.
                  [006180]   295  
                  [006180]   296  ; end of STIG's mod
                  [006180]   297  
                  [006180]   298  	lda	r1, 1(r31)			;  				 
                  [006180]   299  	sll	r1, #ev6__cc_ctl__cc_ena__s, r1	; Move the cc_ena bit into correct position
                  [006180]   300  	hw_mtpr	r1, ev6__cc_ctl			; clear and enable cycle counter
                  [006180]   301  	addq	r31, r31, r31			; nop
                  [006180]   302  	addq	r31, r31, r31			; nop
                  [006180]   303  	addq	r31, r31, r31			; nop
                  [006180]   304          hw_mtpr r31, <ev6__mm_stat ! 32> 	; dummy IPR write
                  [006180]   305  						;   sets SCBD bit 5
                  [006180]   306  						;   (to stall untill cc_ctl gets written)
                  [006180]   307  
                  [006180]   308  wait:						; REPEAT 			 
                  [006180]   309  	rpcc	r1				; : read the cycle count 	 
                  [006180]   310  	zapnot	r1, #^x0f, r1			; : clear the high longword 	 
                  [006180]   311  	cmplt	r0, r1, r1			; :  				 
                  [006180]   312  	PVC_VIOLATE <1008>			; EJS -- don't take branch
                  [006180]   313  	beq	r1, wait			; UNTIL we have waited time 	 
                  [006180]   314  
                  [006180]   315  ; STIG - added to preserve the value already accrued in the cycle counter
                  [006180]   316  
                  [006180]   317  wait_cleanup:
                  [006180]   318  	rpcc	r1
                  [006180]   319  	zapnot	r1, #^x0f, r1			; get CC's 32 bits
                  [006180]   320  	addq	r1, r28, r28			; add in this delta
                  [006180]   321  
                  [006180]   322  	; if we overflowed 32 bits, no matter, the effect is lost by the below
                  [006180]   323  
                  [006180]   324  	lda	r1, 1(r31)
                  [006180]   325  	sll	r1, #ev6__cc_ctl__cc_ena__s, r1	; cc_ena bit 
                  [006180]   326  	bis	r1, r28, r28			; to keep counter ticking
                  [006180]   327  
                  [006180]   328  	hw_mtpr	r28, ev6__cc_ctl		; clear and enable cycle counter
                  [006180]   329  	addq	r31, r31, r31			; nop
                  [006180]   330  	addq	r31, r31, r31			; nop
                  [006180]   331  	addq	r31, r31, r31			; nop
                  [006180]   332          hw_mtpr r31, <ev6__mm_stat ! 32> 	; dummy IPR write
                  [006180]   333  						;   sets SCBD bit 5
                  [006180]   334  						;   (to stall until cc_ctl gets written)
                  [006180]   335  						; specified	
                  [006180]   336  
                  [006180]   337  ; end of STIG's mod
                  [006180]   338  
                  [006180]   339  	PVC_VIOLATE <29>
                  [006180]   340  	PVC_JSR wait, bsr=1, dest=1
                  [006180]   341  	ret	r31, (p20)			; RETURN 			 
                  [006180]   342  
                  [006180]   343  
                  [006180]   344  
                  [006180]   345  ; ======================================================================
                  [006180]   346  ; = autobaud_sync -- routine used for synchronizing with the serial line=
                  [006180]   347  ; ======================================================================
                  [006180]   348  ; OVERVIEW:
                  [006180]   349  ;       The serial line is polled until the a start bit is seen.  We then
                  [006180]   350  ;       time from then until the stop bit is detected.  This happens when
                  [006180]   351  ;       the line goes high and stays high for longer than 8 times the 
                  [006180]   352  ;       previous time.
                  [006180]   353  ;
                  [006180]   354  ;      1->0  - is the start bit
                  [006180]   355  ;      0->1  - is bit 0 of "55"h
                  [006180]   356  ;      1->0  - is bit 1 of "55"h
                  [006180]   357  ;      0->1  - is bit 2 of "55"h
                  [006180]   358  ;      1->0  - is bit 3 of "55"h
                  [006180]   359  ;      0->1  - is bit 4 of "55"h
                  [006180]   360  ;      1->0  - is bit 5 of "55"h
                  [006180]   361  ;      0->1  - is bit 6 of "55"h
                  [006180]   362  ;      1->0  - is bit 7 of "55"h
                  [006180]   363  ;      0->1  - is the stop bit
                  [006180]   364  ;  
                  [006180]   365  ; FORM OF CALL:
                  [006180]   366  ;       bsr p7, autobaud_sync
                  [006180]   367  ;
                  [006180]   368  ; REGISTERS:
                  [006180]   369  ;       R0:R2 - Scratch.
                  [006180]   370  ;
                  [006180]   371  ; RETURNS:	
                  [006180]   372  ;       R24  - Timing info for using srom port is saved here.
                  [006180]   373  ;
                  [006180]   374   
                  [006180]   375  autobaud_sync:
                  [006180]   376  	lda	r1, 1(r31)			; r1 <- cc_ctl enable bit 	 
                  [006180]   377  	sll	r1, #ev6__cc_ctl__cc_ena__s, r1	; Move the cc_ena bit into correct position
                  [006180]   378  
                  [006180]   379  	PVC_JSR	key, bsr=1
                  [006180]   380  	bsr	p20, keypressed			; Clear serial line interrupts	 
                  [006180]   381  ;
                  [006180]   382  ; Wait for the start bit. Line will transition from high to low 
                  [006180]   383  ;
                  [006180]   384  
                  [006180]   385  ; STIG - added timeout
                  [006180]   386  ; zap the PCC so it starts from zero, count up to a billion cycles,
                  [006180]   387  ; if nothing has happened in that time (1-2s), return failure
                  [006180]   388  
                  [006180]   389  	lda     r2, 1(r31)			; cc_ena bit makes it tick
                  [006180]   390  	sll     r2, #ev6__cc_ctl__cc_ena__s, r2
                  [006180]   391  
                  [006180]   392  	hw_mtpr r2, EV6__CC_CTL			; clear and enable cycle counter
                  [006180]   393  	addq    r31, r31, r31			; nop
                  [006180]   394  	addq    r31, r31, r31			; nop
                  [006180]   395  	addq    r31, r31, r31			; nop
                  [006180]   396  	hw_mtpr r31, <ev6__mm_stat ! 32>	; dummy IPR write
                  [006180]   397  						;   sets SCBD bit 5 to stall
                  [006180]   398  						;   until cc_ctl gets written
                  [006180]   399  						; specified
                  [006180]   400  	lda	r2, 1(r31)
                  [006180]   401  	sll	r2, #30, r2			; 2^30 = approx 1 billion cycles
                  [006180]   402  
                  [006180]   403  	br	r31, poll_start			; start looking for keypresses
                  [006180]   404  
                  [006180]   405  ; code fragment to handle the case when there is a timeout on the keypress 
                  [006180]   406  
                  [006180]   407  ctr_timeout:
                  [006180]   408  	bis	r31, r31, r24			; indicate a timeout by zero
                  [006180]   409  
                  [006180]   410  	PVC_JSR	auto, bsr=1, dest=1
                  [006180]   411  	PVC_VIOLATE <29>
                  [006180]   412  	ret	r31, (p7)			; return with timeout result
                  [006180]   413  
                  [006180]   414  ; end of STIG 
                  [006180]   415  
                  [006180]   416  
                  [006180]   417  
                  [006180]   418  poll_start:					; DO 				 
                  [006180]   419  	rpcc	r0				; STIG Check elapsed time
                  [006180]   420  	subq	r0, r2, r0			; STIG greater than timeout?
                  [006180]   421  	bge	r0, ctr_timeout			; STIG if so then quit
                  [006180]   422  
                  [006180]   423  	hw_mfpr	r0, ev6__i_ctl			; : r0 <- sl_rcv 
                  [006180]   424  	srl 	r0, #ev6__i_ctl__sl_rcv__s, r0	; : r0<0> <- current data bit
                  [006180]   425  	PVC_VIOLATE <1008>			; EJS -- do not follow branch
                  [006180]   426  	blbs	r0, poll_start			; WHILE sl_rcv remains high
                  [006180]   427  	hw_mtpr	r1, ev6__cc_ctl			; Enable/clear the cycle counter.  
                  [006180]   428  ;
                  [006180]   429  ; Wait for a low to high transition (or the stop bit)  
                  [006180]   430  ;
                  [006180]   431  poll_h:                   			; : REPEAT 			 
                  [006180]   432  	hw_mfpr	r0, ev6__i_ctl			; : r0 <- sl_rcv 		 
                  [006180]   433  	srl 	r0, #ev6__i_ctl__sl_rcv__s, r0	; : r0<0> <- current data bit
                  [006180]   434  	PVC_VIOLATE <1008>			; EJS -- do not follow branch	 
                  [006180]   435          blbc	r0, poll_h			; : UNTIL sl_rcv goes HI 	 
                  [006180]   436  
                  [006180]   437  	rpcc	r1				; Record low-to-hi transition.  
                  [006180]   438  	zap	r1, #^xF0, r1			; blow off the high longword	 
                  [006180]   439  ;	
                  [006180]   440  ; Wait for a high to low transition or time out  
                  [006180]   441  ;
                  [006180]   442  poll_l:						; : REPEAT 			 
                  [006180]   443  	rpcc	r0
                  [006180]   444  	zap	r0, #^xF0, r0			; blow off the high longword	 
                  [006180]   445  	sll	r1, #3, r2			; 8 times low-to-high time.	 
                  [006180]   446  	cmpult  r2, r0, r0			; Have reached stop bit?	 
                  [006180]   447  	bne	r0, end_sync
                  [006180]   448  	hw_mfpr	r0, ev6__i_ctl			; : r0 <- sl_rcv 		 
                  [006180]   449  	srl 	r0, #ev6__i_ctl__sl_rcv__s, r0	; : r0<0> <- current data bit
                  [006180]   450  	PVC_VIOLATE <1008>			; EJS -- do not follow branch	 
                  [006180]   451          blbs	r0, poll_l			; : UNTIL sl_rcv goes LO 	
                  [006180]   452  	PVC_VIOLATE <1008> 
                  [006180]   453  	br	r31, poll_h			; Wait for next low-to-hi trans
                  [006180]   454  
                  [006180]   455  end_sync:
                  [006180]   456  	lda	r0, 0(r31)			; Divide total cycle count (r1) 
                  [006180]   457  simple_div:
                  [006180]   458  	mulq	r0, #9, r2			; by 9 (8 data + 1 start bit)	 
                  [006180]   459  	cmpult	r2, r1, r2			; This is a simple divide.
                  [006180]   460  	beq	r2, set_baud			; R0 = R1/9
                  [006180]   461  	addq	r0, #1, r0
                  [006180]   462  	br	r31, simple_div
                  [006180]   463  
                  [006180]   464  set_baud:
                  [006180]   465  	sll	r0,  #32, r0			; move to hi longword 		 
                  [006180]   466  	lda	r24, mc_m_echo(r0)		; r24 <- cycles-per-bit  	 
                  [006180]   467  
                  [006180]   468  	PVC_JSR	auto, bsr=1, dest=1
                  [006180]   469  	PVC_VIOLATE <29>
                  [006180]   470  	ret	r31, (p7)
                  [006180]   471  
                  [006180]   472  .endm	SROM_IO_FUNCTIONS
