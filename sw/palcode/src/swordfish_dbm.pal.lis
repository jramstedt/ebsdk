                  [000000]   511  .macro	PVC_JSR token, dest=0, bonus=0, bsr=0
                  [000000]   512      .iif gt bsr-1, pcv__'token' == bsr
                  [000000]   513      .if ndf osfpal
                  [000000]   514  	.iif ndf pcv_jsr_index, pcv_jsr_index = 2100
